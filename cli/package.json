{
  "name": "rowell-cli",
  "version": "1.0.0",
  "description": "Rowell Infra CLI - Command line tool for managing Rowell Infra projects",
  "main": "dist/index.js",
  "type": "commonjs",
  "bin": {
    "rowell": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "test": "jest",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build"
  },
  "keywords": [
    "stellar",
    "hedera",
    "blockchain",
    "africa",
    "fintech",
    "remittance",
    "stablecoin",
    "cli",
    "tool"
  ],
  "author": "Rowell Infra Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/rowell-infra/rowell-infra.git",
    "directory": "cli"
  },
  "bugs": {
    "url": "https://github.com/rowell-infra/rowell-infra/issues"
  },
  "homepage": "https://github.com/rowell-infra/rowell-infra#readme",
  "dependencies": {
    "commander": "^11.0.0",
    "inquirer": "^9.2.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.0",
    "axios": "^1.6.0",
    "fs-extra": "^11.1.0",
    "yaml": "^2.3.0"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@types/inquirer": "^9.0.0",
    "@types/fs-extra": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "prettier": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "files": [
    "dist/**/*",
    "README.md",
    "LICENSE"
  ],
  "engines": {
    "node": ">=16.0.0"
  }
}
